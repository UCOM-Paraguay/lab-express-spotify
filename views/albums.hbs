<div class="container">
    <h1>Albumes</h1>
  <div class="artists">
    {{#each items}}
        <div class="item">
            <div class="img-item">
                <img src="{{images.[0].url}}" alt="artist image">
            </div>
            <div class="item-body">
                <h3>{{name}}</h3>
                <a href="/tracks/{{id}}/{{name}}/" ><button class="btn btn-danger">Ver canciones</button></a>
            </div>
        </div>
    {{/each}}
  </div>
</div>